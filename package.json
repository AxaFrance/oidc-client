{
  "name": "@axa-fr/react-oidc-root",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AxaGuilDEv/react-oidc.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "version": "lerna version",
    "setnpmtoken": "node ./scripts/npm.js",
    "publish": "lerna publish",
    "example:redux": "npm -C ./examples/redux run start",
    "example:context": "npm -C ./examples/context run start",
    "test": "jest --config=jest.config.js",
    "lint": "npm run lint:js && npm run lint:ts **/*.ts",
    "lint:js": "cross-env NODE_ENV=production eslint . --cache --cache-location=.cache/eslint --ext .js,.jsx,.json --report-unused-disable-directives",
    "lint:ts": "tslint -p . -c tslint.json -t stylish",
    "dev": "concurrently --kill-others \"yarn dev:ts\" \"yarn dev:js\"",
    "dev:js": "lerna exec --parallel -- cross-env-shell node \\$LERNA_ROOT_PATH/scripts/watch-js.js",
    "dev:ts": "lerna exec --parallel -- cross-env-shell node \\$LERNA_ROOT_PATH/scripts/watch-ts.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "npm run lint:js -- --fix",
        "git add"
      ],
      "*.ts": [
        "npm run lint:ts -- --fix",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-macros": "^2.4.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "codelyzer": "^4.4.4",
    "commander": "^2.17.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^23.5.0",
    "inquirer": "^6.2.0",
    "jest": "^23.5.0",
    "lerna": "3.1.0",
    "lint-staged": "^7.2.2",
    "npmlog": "^4.1.2",
    "oidc-client": "^1.5.1",
    "prettier": "^1.14.0",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-redux": "~5.0.7",
    "react-test-renderer": "^16.4.2",
    "recompose": "~0.30.0",
    "redux": "^4.0.0",
    "redux-oidc": "^3.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "shelljs": "^0.8.2",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.3"
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.3.2"
  },
  "private": true
}
