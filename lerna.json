{
  "version": "1.3.3",
  "command": {
    "publish": {
      "message": "chore(release): publish %s ***NO_CI***",
      "npmClient": "npm",
      "ignoreChanges": ["ignored-file", "*.md", ".npmrc"]
    },
    "bootstrap": {
      "ignore": "component-*",
      "npmClientArgs": ["--no-package-lock"]
    }
  },
  "packages": ["examples/*", "packages/*"]
}
