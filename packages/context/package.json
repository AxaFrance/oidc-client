{
  "name": "@axa-fr/react-oidc-context",
  "version": "4.0.0-alpha.17",
  "private": false,
  "main": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "description": "OpenID Connect & OAuth authentication using react context api as state management",
  "repository": {
    "type": "git",
    "url": "https://github.com/AxaGuilDEv/react-oidc.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "oidc",
    "context-api",
    "OpenID Connect",
    "openid",
    "oauth2",
    "react-component"
  ],
  "scripts": {
    "copy": "copyfiles -f ./src/oidc/vanilla/OidcServiceWorker.js ./public && copyfiles -f -soft ./src/oidc/vanilla/OidcTrustedDomains.js ./public",
    "start": "npm run copy && set PORT=5002 && react-scripts start",
    "build": "npm run copy && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf dist",
    "prepare": "npm run clean && tsc --build \"./tsconfig.json\" && copyfiles -f ./src/oidc/vanilla/OidcServiceWorker.js ./dist && copyfiles -f ./src/oidc/vanilla/OidcTrustedDomains.js ./dist"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "dependencies": {
    "@openid/appauth": "1.3.1"
  },
  "devDependencies": {
    "copyfiles": "2.4.1",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "bootstrap": "4.6.1",
    "react-router-dom": "5.3.0",
    "react-scripts": "5.0.0",
    "typescript": "4.5.5"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
